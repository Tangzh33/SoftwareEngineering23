{
  "name": "react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "6.14.6",
    "node": "12.18.4"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@mui/material": "^5.8.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "ace-builds": "^1.5.1",
    "query-string": "^6.14.1",
    "react": "^18.1.0",
    "react-ace": "^9.3.0",
    "react-dom": "^18.1.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.2.0",
    "react-rte": "^0.16.3",
    "react-scripts": "^3.4.3",
    "react-typical": "^0.1.3",
    "tailwindcss": "^1.4.6"
  },
  "scripts": {
    "purge:css": "cross-env NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "start": "npm run build:css && react-scripts start",
    "build": "npm run purge:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.1"
  }
}
